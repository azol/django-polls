{
  "builds": [
    {
      "src": "mysite/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "vercel-static-build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static"
      }
    }
 ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "mysite/wsgi.py"
    }
  ]
}
